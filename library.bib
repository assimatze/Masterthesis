journal = {Business and Information  Systems Engineering},
keywords = {Gamification. Design of IT- Based Enhancing Servic},
number = {4},
pages = {275--278},
pmid = {18605031},
primaryClass = {ACM 978-1-4503-0816-8},
title = {{Gamification: Design of IT-based enhancing services for motivational support and behavioral change}},
